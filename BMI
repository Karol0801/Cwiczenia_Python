def BMI_Calculator(weight, height):
    BMI = weight / pow(height, 2)
    if BMI < 18.5:
        print("Twoje BMI jest za niskie")
    elif BMI < 25:
        print("Twoje BMI jest odpowiednie")
    else:
        print("Twoje BMI jest zbyt wysokie")
        
w, h = input("Enter your weight [kg] and height[m]: ").split()
BMI_Calculator(float(w), float(h))

